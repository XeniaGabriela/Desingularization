proc testideal(i) {
	if(i == 0)
	{
		ideal result=1;
		return(result);
	}
	i=std(i);
        int ht=nvars(basering)-dim(i);
	ideal j=minor(jacob(i),ht);
	ideal tmp=i,j;
	ideal result=radical(tmp);
	return(result);
};
